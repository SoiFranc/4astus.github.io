{
  "name": "4as-t-us",
  "version": "1.0.0",
  "description": "Site vitrine association de poker 4As-t-us",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch src/scss:src/css",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "build:sass": "sass src/css:public/css",
    "build:eleventy": "npx @11ty/eleventy",
    "startc": "npm run watch:sass",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:sass --parallel build:*",
    "0ld-starta": "npm run watch:sass && npm run watch:eleventy",
    "0ld-builda": "npm run build:sass && npm run build:eleventy",
    "0ld-build:mincss": "npx postcss style.css --use cssnano -o public/css/style.min.css",
    "0ld-build:elevent": "npx @11ty/eleventy",
    "0ld-def": "npm-run-all build:sass --parallel watch:*",
    "0ld-deff": "npm-run-all build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SoiFranc/4As-t-us.git"
  },
  "author": "SoiFranc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SoiFranc/4As-t-us/issues"
  },
  "homepage": "https://github.com/SoiFranc/4As-t-us#readme",
  "dependencies": {
    "bootstrap": "^5.1.3",
    "cssnano": "^5.1.9",
    "the-new-css-reset": "^1.6.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.52.1"
  }
}
